{
  "presets": [
    [
      "@babel/env",
      {
        "debug": true,
        "corejs": 3,
        "targets": [
          "last 5 versions"
        ],
        "useBuiltIns": "usage"
      }
    ]
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/plugin-transform-regenerator"
  ]
}
